{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "crons": [
    {
      "path": "/notifications/push/worker/run",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/notifications/campaign/daily/run",
      "schedule": "30 3 * * *"
    }
  ]
}
